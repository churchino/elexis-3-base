<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="at.medevit.elexis.medicationlist.ui.MedicationView"
            icon="icon://IMG_PILL"
            id="at.medevit.elexis.medicationlist.ui.medicationlist.view"
            name="%view.name">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:at.medevit.elexis.medicationlist.ui.medicationlist.view">
         <command
               commandId="at.medevit.elexis.medicationlist.ui.command.printEtiquette"
               icon="icon://IMG_PRINTER"
               style="push">
         </command>
         <command
               commandId="at.medevit.elexis.medicationlist.ui.command.PrintRecipe"
               icon="icon://IMG_PRINTER"
               style="push">
         </command>
         <command
               commandId="at.medevit.elexis.swissmedic.search"
               style="push">
         </command>
         <command
               commandId="at.medevit.elexis.epha.interactions.ephasearch"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:at.medevit.elexis.medicationlist.ui.medicationlist.view">
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="at.medevit.elexis.medicationlist.ui.command.printEtiquette"
            name="%command.printEtiquette.name">
      </command>
      <command
            defaultHandler="at.medevit.elexis.medicationlist.ui.command.PrintRecipeHandler"
            id="at.medevit.elexis.medicationlist.ui.command.PrintRecipe"
            name="%command.printRecipe.name">
      </command>
      <command
            id="at.medevit.elexis.medicationlist.ui.command.toggleHandOverType"
            name="%command.toggleHandOverType.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="at.medevit.elexis.medicationlist.ui.command.ToggleHandOverTypeHandler"
            commandId="at.medevit.elexis.medicationlist.ui.command.toggleHandOverType">
      </handler>
   </extension>
</plugin>
